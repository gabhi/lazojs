{
    "directories": {
        "application": "application",
        "components": "components",
        "models": "models"
    },
    "server": {
        "instances": {
            "primary": {
                "port": 8080
            }
        },
        "defaults": {
            "timeout": {
                "server": 30000,
                "client": 30000
            },
            "maxBytes": 10485760,
            "debug": false,
            "security": true,
            "state": {
                "cookies": {
                    "failAction": "log",
                    "strictHeader": false
                }
            }
        },
        "maxSockets": 500
    },
    "client": {},
    "requirejs": {
        "shim": {
            "handlebars": {
                "exports": "Handlebars"
            }
        },
        "paths": {}
    },
    "libPath": "/lib/optimized/lib.js"
}
